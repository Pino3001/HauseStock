<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/Home_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HauseStock.NoActionBar">
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/lista_scene" />

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".Lista.Lista"
            android:screenOrientation="portrait"/>
        <activity android:name=".AgregarStock.Agregar"
            android:screenOrientation="portrait"/>
        <activity android:name=".VerStocks.VerStock" />
        <activity android:name=".Vencimientos.VerVencimientos"/>
    </application>

</manifest>